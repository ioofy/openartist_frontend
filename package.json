{
  "name": "yuk-beli",
  "description": "An app for react marketplace based on typescript",
  "version": "1.0.0",
  "private": true,
  "author": "Ioofy (Kyy)",
  "license": "MIT",
  "keywords": [
    "react-cdd",
    "react-shop"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "prepare": "husky install"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.8.1",
    "classnames": "2.3.1",
    "graphql": "16.4.0",
    "next": "12.1.5",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "redux": "4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@emotion/babel-plugin": "11.9.2",
    "@emotion/core": "11.0.0",
    "@emotion/css": "11.9.0",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@types/node": "16.11.27",
    "@types/react": "18.0.8",
    "@types/react-dom": "18.0.2",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "antd": "4.20.2",
    "autoprefixer": "10.4.6",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.4.1",
    "postcss": "8.4.13",
    "prettier": "2.6.2",
    "tailwindcss": "3.0.24",
    "typescript": "4.6.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
